<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:orientation="vertical">
  
  <TextView android:id="@+id/t_logTop"
    android:layout_width="fill_parent"
    android:layout_height="0dip"
    android:layout_weight=".4"
    android:text="Result from Get"
    android:scrollbars="vertical"
    android:maxLines="15"/>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal" >

      <TextView
          android:id="@+id/textView1"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="Id:" />

      <EditText
          android:id="@+id/canId"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight=".6"
          android:ems="10"
          android:inputType="number"
          android:singleLine="true"
          android:hint="Identifier"
          android:selectAllOnFocus="true"
          android:text="100"
          android:nextFocusDown="@+id/dlc" >
      </EditText>
      
      <CheckBox
        android:id="@+id/chk_extended"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="2dp"
        android:text="Ext " />    

	<TextView
          android:id="@+id/textView2"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="DLC:" />

      <EditText
          android:id="@id/dlc"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight=".3"
          android:ems="10"
          android:inputType="number"
          android:maxLength="2"
          android:selectAllOnFocus="true"
          android:hint="dlc"
          android:text="0"
          android:nextFocusDown="@+id/data00" />

   </LinearLayout>
	  	  

    <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal" >

 	      <TextView
 	      	android:layout_width="wrap_content"
 	        android:layout_height="wrap_content"
 	        android:text="D0:" />

	      <EditText
	        android:id="@id/data00"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
            android:nextFocusDown="@+id/data01" />
        
	      <TextView
 	      	android:layout_width="wrap_content"
 	        android:layout_height="wrap_content"
 	        android:text="D1:" />

	      <EditText
	        android:id="@id/data01"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
            android:nextFocusDown="@+id/data02" />
 
	      <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="D2:" />

	      <EditText
	        android:id="@id/data02"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
            android:nextFocusDown="@+id/data03"
	         />
	      
	       <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="D3:" />

	      <EditText
	        android:id="@id/data03"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
            android:nextFocusDown="@+id/data04"
	         />	
  </LinearLayout>
    
    <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal" >

 	      <TextView
 	      	android:layout_width="wrap_content"
 	        android:layout_height="wrap_content"
 	        android:text="D4:" />

	      <EditText
	        android:id="@id/data04"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
            android:nextFocusDown="@+id/data05" />
        
	      <TextView
 	      	android:layout_width="wrap_content"
 	        android:layout_height="wrap_content"
 	        android:text="D5:" />

	      <EditText
	        android:id="@id/data05"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
            android:nextFocusDown="@+id/data06" />
 
	      <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="D6:" />

	      <EditText
	        android:id="@id/data06"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
            android:nextFocusDown="@+id/data07"
	         />
	      
	       <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="D7:" />

	      <EditText
	        android:id="@id/data07"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
 	        android:layout_weight=".1"
	        android:ems="10"
	        android:inputType="number"
          	android:selectAllOnFocus="true"
	        android:text="0"
	         />	
  </LinearLayout>
  
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal" >
        
	<Button
	    android:id="@+id/b_send"
	    android:layout_height="wrap_content"
	    android:layout_width="0dp"
	    android:layout_weight=".4"
	    android:text="Send" 
	    />

      	  
   </LinearLayout>  
    
  <TextView android:id="@+id/t_logBottom"
  android:layout_width="fill_parent"
  android:layout_height="0dip"
  android:layout_weight=".2"
  android:text="Test To See if Post working"
  android:scrollbars="vertical"
  android:maxLines="15"
  android:gravity="bottom"/>
  
  
</LinearLayout>
